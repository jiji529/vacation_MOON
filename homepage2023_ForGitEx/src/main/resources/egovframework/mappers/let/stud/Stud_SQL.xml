<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="egovframework.let.stud.service.impl.StudMapper">

	<resultMap id="stud" type="egovframework.let.stud.service.StudVO">
		<result property="studNum" column="STUD_NUM"/>
		<result property="studName" column="STUD_NAME"/>
	</resultMap>

	<select id="selectStud" resultMap="stud">
			SELECT
				STUD_NUM
				, STUD_NAME
			FROM letstud
			WHERE STUD_NUM = #{studNum}
	</select>
	
